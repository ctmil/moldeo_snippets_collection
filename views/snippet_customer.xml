<odoo>
  <data>

      <!--Customers Snippets-->
      <template id="snippet_customer_id" inherit_id="website.snippets" name="Customer Snippet ID">
        <xpath expr="//div[@id='snippet_structure']/div[@class='o_panel_body']" position="inside">
            <t t-snippet="moldeo_snippets_collection.snippet_customer"
              t-thumbnail="data:image/png;base64,"/>
        </xpath>
      </template>

      <template id="snippet_customer" name="Customers Snippet">
          <section class="moldeo_snippet_customer">
            <div class="container pt-3 pb-3">
              <h3>
                Customers
              </h3>
            
              <div class="row mt-4" id="snippet_customer_container" itemscope="" itemtype="http://schema.org/Organization">
                    <div class="col-12 text-muted text-center">Here will show customer.</div>
              </div>
            </div>
          </section>
      </template>
      
  </data>
</odoo>